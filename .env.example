# ===========================================
# GigHub Environment Variables
# ===========================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control!
# ===========================================

# Supabase Configuration
# Get these from: https://supabase.com/dashboard → Project Settings → API
SUPABASE_URL="https://your-project-id.supabase.co"
SUPABASE_ANON_KEY="your-anon-key-here"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key-here"
SUPABASE_JWT_SECRET="your-jwt-secret-here"
SUPABASE_PUBLISHABLE_KEY="your-publishable-key-here"
SUPABASE_SECRET_KEY="your-secret-key-here"

# Angular Environment Variables (prefixed with NG_)
NG_PUBLIC_SUPABASE_URL="https://your-project-id.supabase.co"
NG_PUBLIC_SUPABASE_ANON_KEY="your-anon-key-here"
NG_PUBLIC_SUPABASE_PUBLISHABLE_KEY="your-publishable-key-here"

# PostgreSQL Configuration
# Get these from: https://supabase.com/dashboard → Project Settings → Database
POSTGRES_HOST="db.your-project-id.supabase.co"
POSTGRES_USER="postgres"
POSTGRES_PASSWORD="your-postgres-password"
POSTGRES_DATABASE="postgres"
POSTGRES_URL="postgres://postgres.your-project-id:your-password@aws-0-region.pooler.supabase.com:6543/postgres?sslmode=require&supa=base-pooler.x"
POSTGRES_URL_NON_POOLING="postgres://postgres.your-project-id:your-password@aws-0-region.pooler.supabase.com:5432/postgres?sslmode=require"
POSTGRES_PRISMA_URL="postgres://postgres.your-project-id:your-password@aws-0-region.pooler.supabase.com:6543/postgres?sslmode=require&pgbouncer=true"

# ===========================================
# Setup Instructions:
# 1. Copy this file: cp .env.example .env
# 2. Fill in your Supabase credentials in .env
# 3. Run database setup: see docs/database/README.md
# ===========================================

